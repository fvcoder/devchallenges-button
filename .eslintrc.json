{
  "$schema": "https://json.schemastore.org/eslintrc.json",
  "extends": ["eslint-config-codely/typescript"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx"],
      "parserOptions": {
        "project": ["./tsconfig.json"]
      }
    }
  ],
  "rules": {
    "prettier/prettier": ["error", { "printWidth": 100, "useTabs": false, "endOfLine": "crlf" }]
  }
}
